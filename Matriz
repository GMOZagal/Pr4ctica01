#include <iostream>
#include <stdlib.h>
#include <time.h>


int main (){

//declarar matriz
int a[3][3]{1,2,3,4,5,6,7,8,9};
int b[3][3]{1,2,3,4,5,6,7,8,9};
int c[3][3];

//recorrer casillas
  for (int i=0; i<4; i++) {
      for (int j=0; j<4; j++) {
        printf( "%d",& a[i][j]);
        }
     }
  for (int i=0; i<4; i++) {
      for (int j=0; j<4; j++) {
        printf( "%d",& b[i][j]);
        }
     }
     
//hacer la suma de las casillas correspondientes
  for (int i=0; i<4; i++) {
      for (int j=0; j<4; j++) {
      (c[i][j]= a[i][j]+b[i][j]); {
        printf( "%d",& a[i][j]);
        }
        }
     }
     
retun 0;}
